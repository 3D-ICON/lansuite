{
    "name": "lansuite/lansuite",
    "description": "A Content Management System designed especially for the needs of LAN-Parties",
    "type": "project",
    "keywords": ["lanparty", "cms", "gaming", "esports"],
    "homepage": "https://github.com/lansuite/lansuite",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "LanSuite-Team",
            "homepage": "https://github.com/lansuite/lansuite/graphs/contributors"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-gd": "*",
        "ext-mysqli": "*",

        "smarty/smarty": "^3.1",
        "geshi/geshi": "^1.0.9",
        "setasign/fpdf": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "doctrine/instantiator": "~1.0.5"
    },
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin"
    },
    "suggest": {
        "ext-snmp": "Required to use the \"Network Operation Center\" module."
    },
    "autoload": {
        "psr-4": {
            "LanSuite\\Module\\Beamer\\": "modules/beamer/Classes/",
            "LanSuite\\Module\\CashMgr\\": "modules/cashmgr/Classes/",
            "LanSuite\\Module\\Foodcenter\\": "modules/foodcenter/Classes/",
            "LanSuite\\Module\\GuestList\\": "modules/guestlist/Classes/",
            "LanSuite\\Module\\MasterSearch2\\": "modules/mastersearch2/Classes/",
            "LanSuite\\Module\\PDF\\": "modules/pdf/Classes/",
            "LanSuite\\Module\\Seating\\": "modules/seating/Classes/",
            "LanSuite\\": "inc/Classes"
        },
        "files": [
            "inc/Functions/T.php",
            "inc/Functions/FetchDataRow.php",
            "inc/Functions/FetchPostRow.php",
            "inc/Functions/MasterCommentEditAllowed.php",
            "inc/Functions/CheckValidEmail.php",
            "inc/Functions/Debug.php",
            "inc/Functions/DependencyPreCheck.php",
            "modules/about/Functions/GetTheLinesAndChars.php",
            "modules/about/Functions/LetsOpenTheDir.php",
            "modules/board/Functions/FormatTitle.php",
            "modules/board/Functions/GetBoardRank.php",
            "modules/board/Functions/GetUserInfo.php",
            "modules/board/Functions/LastPostDetails.php",
            "modules/board/Functions/LastPostDetailsShow.php",
            "modules/board/Functions/NameAndDesc.php",
            "modules/board/Functions/NewPosts.php",
            "modules/board/Functions/Update.php",
            "modules/guestlist/Functions/ClanURLLink.php",
            "modules/guestlist/Functions/GetLastHitUrl.php",
            "modules/guestlist/Functions/GetModul.php",
            "modules/guestlist/Functions/GetTimeDiff.php",
            "modules/guestlist/Functions/GetTimeDiffAsName.php",
            "modules/guestlist/Functions/PaidIconLink.php",
            "modules/guestlist/Functions/PPrice.php",
            "modules/guestlist/Functions/SeatNameLink.php",
            "modules/mastersearch2/Functions/MS2GetDate.php",
            "modules/mastersearch2/Functions/MS2GetTime.php",
            "modules/mastersearch2/Functions/Text2LSCode.php",
            "modules/mastersearch2/Functions/TrueFalse.php",
            "modules/mastersearch2/Functions/UserNameAndIcon.php"
        ]
    }
}
